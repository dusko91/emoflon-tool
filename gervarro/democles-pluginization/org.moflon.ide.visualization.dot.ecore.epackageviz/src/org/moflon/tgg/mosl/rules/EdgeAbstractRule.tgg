#using org.moflon.ide.visualization.dot.ecore.epackageviz.*
#using AttrCondDefLibrary.*

#abstract #rule EdgeAbstractRule #extends EdgeAbstractSourceOnlyRule #with org.moflon.ide.visualization.dot.ecore.epackageviz

#source {
	graph:ClassGraph

	trgClassNode:PNode{
		- graph -> graph
	}
}

#target {
	ePackage:EPackage

	trgClass:EClass{
		- ePackage -> ePackage
	}
}

#correspondence {
	graphToPackage :ClassGraphToEPackage {
		#src->graph
		#trg->ePackage
	}

	trgNodeToClassifier :PNodeToEClassifier{
		#src->trgClassNode
		#trg->trgClass
	}
}

#attributeConditions {
	
}


